<!DOCTYPE html>
<html lang="de">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 2 | Dashboard</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">


    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">


    {% block stylesheets %}
        {% stylesheets
        'assets/vendor/css/bootstrap.min.css'
        'assets/vendor/dist/css/AdminLTE.min.css'
        'assets/vendor/dist/css/skins/_all-skins.min.css'
        'assets/vendor/plugins/iCheck/flat/blue.css'
        'assets/vendor/plugins/morris/morris.css'
        'assets/vendor/plugins/jvectormap/jquery-jvectormap-1.2.2.css'
        'assets/vendor/plugins/datepicker/datepicker3.css'
        'assets/vendor/plugins/daterangepicker/daterangepicker-bs3.css'
        'assets/vendor/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
    {% endblock %}
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
    <body class="skin-blue fixed">
    {% block header %}
    {% endblock %}
        {% block body %}{% endblock %}
        {% block footer %}

        {% endblock %}
        {% block javascripts %}

            {% javascripts
            'assets/vendor/plugins/jQuery/jQuery-2.1.4.min.js'
            'assets/vendor/js/bootstrap.min.js'
            'assets/vendor/plugins/morris/morris.min.js'
            'assets/vendor/plugins/sparkline/jquery.sparkline.min.js'
            'assets/vendor/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js'
            'assets/vendor/plugins/jvectormap/jquery-jvectormap-world-mill-en.js'
            'assets/vendor/plugins/knob/jquery.knob.js'
            'https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.2/moment.min.js'
            'assets/vendor/plugins/daterangepicker/daterangepicker.js'
            'assets/vendor/plugins/datepicker/bootstrap-datepicker.js'
            'assets/vendor/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js'
            'assets/vendor/plugins/slimScroll/jquery.slimscroll.min.js'
            'assets/vendor/plugins/fastclick/fastclick.min.js'
            'assets/vendor/dist/js/app.js'
            'assets/vendor/dist/js/pages/dashboard.js'
            'assets/vendor/dist/js/demo.js' %}
            <script src="{{ asset_url }}"></script>
            {% endjavascripts %}

            <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
            <script>
                $.widget.bridge('uibutton', $.ui.button);
            </script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>


        {% endblock %}
    </body>
</html>
